module smallALU(
    in_0,
    in_1,
    out
);

input [7: 0] in_0;
input [7: 0] in_1;
output [7: 0] out;

wire [7: 0] out_12, out_21;
wire cout_12, cout_21;

FS_8    FS_EXP_12(.a(in_0), .b(in_1), .out(out_12), .cout(cout_12));
FS_8    FS_EXP_21(.a(in_1), .b(in_0), .out(out_21), .cout(cout_21));

if (cout_12 == 1)
    assign out = cout_21;
    else assign out = cout_12;

endmodule